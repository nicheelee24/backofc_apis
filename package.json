{
  "name": "starter-template",
  "version": "1.0.0",
  "description": "Basic template to start a nodejs ts project",
  "main": "index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "copy-templates": "xcopy public dist\\public /E /I",
    "start": "cross-env NODE_ENV=development nodemon dist/index.js",
    "start:node": "cross-env NODE_ENV=development node dist/index.js",
    "start:prod": "npm run build && cross-env NODE_ENV=production node dist/index.js",
    "start:dev": "npm run build && cross-env NODE_ENV=development  node dist/index.js",
    "start:local": "npm run build && cross-env NODE_ENV=development  nodemon dist/index.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^8.1.3",
    "amqplib": "^0.10.4",
    "aws-sdk": "^2.1626.0",
    "bcryptjs": "^2.4.3",
    "cli-table": "^0.3.11",
    "colorette": "^2.0.20",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "express-validator": "^7.1.0",
    "https": "^1.0.0",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.4.4",
    "mongoose": "^8.4.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.13",
    "redis": "^4.6.14",
    "supertest": "^7.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cli-table": "^0.3.4",
    "@types/cors": "^2.8.17",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/luxon": "^3.4.2",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.12",
    "@types/nodemailer": "^6.4.15",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6"
  }
}
